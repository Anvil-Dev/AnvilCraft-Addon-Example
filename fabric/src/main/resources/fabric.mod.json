  "version": "${version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [],
  "contact": {},

  "license": "${mod_license}",
  "icon": "assets/anvilcraft_addon_example/textures/icon.png",

  "environment": "*",
