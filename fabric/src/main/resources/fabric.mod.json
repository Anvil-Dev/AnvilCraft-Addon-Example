    "main": [
      "dev.anvilcraft.addon.fabric.ExampleModFabric"
    ],
    "fabric-datagen": [
      "dev.anvilcraft.addon.data.fabric.ExampleModDatagen"
    ]
  },
  "mixins": [
    "anvilcraft_addon_example.mixins.json",
    "anvilcraft_addon_example-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.7",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "anvilcraft": ">=1.1"
  }
}
